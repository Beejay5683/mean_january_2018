<html>
	<head>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script type ="text/javascript">
    </script>
	</head>
    <body>
       <div id="wrapper">
        <% if(typeof(errors) != 'undefined' ) { %>
        <% for (var x in errors) { %>
        <h3><%= errors[x].message %></h3>
        <% } %>
        <% } %>
       	<form action="/quotes" id="form" method="post">
       		<p>Your Name: <input name="name" type="text"></p>
       		<p>Quote: <input name="quote" type="text"></p>
       		<input type="submit" value="Add My Quote">
       	</form>
        <a href="/quotes">Skip to quotes</a>

       </div>
	</body>
</html>